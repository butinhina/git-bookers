<%= form_with model: @book,url: update_book_path(@book.id),method: :patch do |f| %>
<div id="edit-title">
<h4>Title</h4>
<%= f.text_field :title %>
</div>
<div id="edit-title">
<h4>Body</h4>
<%= f.text_area :body %>


</div>
<div class="actions">
<%= f.submit "Update Book" %>
<% end %>
</div>

<div class="edit-list">
  <%= link_to "Show",book_path(@book.id) %>
  <%= link_to "Back",'/books' %>
</div>
